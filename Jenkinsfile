pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
   
    
 
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
 
    
  
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
   
    
   
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    
    
   
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
    
}
